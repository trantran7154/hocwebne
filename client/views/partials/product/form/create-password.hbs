<el-form :model="createPasswordForm" 
         :rules="createPasswordValidate" 
         ref="createPasswordForm" 
         label-width="120px" 
         class="demo-ruleForm"
         :label-position="labelPosition">
    <el-form-item label="Chọn chức năng">
      <el-select class="w-100" v-model="createPasswordForm.btn" multiple placeholder="Chọn người được phép truy cập">
        <el-option
          v-for="item in listData.optionsBtn"
          :key="item.value"
          :label="item.label"
          :value="item.value">
        </el-option>
      </el-select>
    </el-form-item>
    <el-form-item label="Chọn người được phép truy cập">
      <el-select class="w-100" v-model="createPasswordForm.useraccess" multiple placeholder="Chọn người được phép truy cập">
        <el-option
          v-for="item in listData.optionsUser"
          :key="item.value"
          :label="item.label"
          :value="item.value">
        </el-option>
      </el-select>
    </el-form-item>
    <el-form-item label="Người được phép đổi mật khẩu truy cập">
      <el-select class="w-100" v-model="createPasswordForm.usereditpass" multiple placeholder="Chọn người được phép đổi mật khẩu">
        <el-option
          v-for="item in listData.optionsUser"
          :key="item.value"
          :label="item.label"
          :value="item.value">
        </el-option>
      </el-select>
    </el-form-item>
    <el-form-item label="Mật khẩu">
      <el-input placeholder="Vui lòng nhập mật khẩu" v-model="createPasswordForm.password" show-password></el-input>
    </el-form-item>
    <el-form-item label="Xác nhận mật khẩu">
      <el-input placeholder="Vui lòng xác nhận mật khẩu" v-model="createPasswordForm.repassword" show-password></el-input>
    </el-form-item>
    <el-form-item>
      <el-button type="primary" @click="createPasswordCreate('createPasswordForm')">Xác nhận</el-button>
    </el-form-item>
</el-form>